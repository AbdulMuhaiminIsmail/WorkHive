{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["**/.env"],
    "pipeline": {
      "dev": {
        "cache": false,
        "persistent": true,
        "outputs": []
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "build/**"]
      }
    }
  }
  